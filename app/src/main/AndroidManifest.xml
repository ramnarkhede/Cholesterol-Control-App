<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cholesterolcontrol">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_exercise"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CholesterolControl">
        <activity
            android:name=".WorkoutActivity"
            android:exported="false"
            android:noHistory="true"/>
        <activity
            android:name=".AddTestActivity"
            android:exported="false" />
        <activity
            android:name=".DayResultActivity"
            android:exported="false"
            android:noHistory="true" />
        <activity
            android:name=".CheckUpHistory"
            android:exported="false"
            android:noHistory="true"/>
        <activity
            android:name=".FatConsumedActivity"
            android:exported="false"
            android:noHistory="true"/>
        <activity
            android:name=".DailyFoodActivity"
            android:exported="false"
            android:noHistory="true"/>
        <activity
            android:name=".MainActivity"
            android:exported="true"
           >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>